CALL M_KOJO_MESSAGE_COM_K33_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K33_140_1
;-------------------------------------------------
;기입체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본세트
	IF FLAG:70
		CALL K33_TATARI_COUNTER(3)
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;굴복3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;그 외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

