				RETURN 1
			ENDIF
		ELSE
			PRINTDATAL
				DATAFORM 「츄우……후……으ー읍!」
				DATAFORM 「응읏…… 츄……우ー」
				DATAFORM 「으ー읍!　으응…… 후으%UNICODE(0x2764)%」
			ENDDATA
			RETURN 1
		ENDIF
		RETURN 1
	;그 외
	ELSE
		PRINTFORMW 「응읏…… 츄……우ー」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

